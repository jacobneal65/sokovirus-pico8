--box movement
function addbox(btle,bx,by) 
	local b={
		x=bx,
		y=by,
		ox=0,
		oy=0,
		tle=btle
	}
	add(box,b)
end

function checkbox(x,y)
	for b in all(box) do
 	if b.x==x and b.y==y then
 		return b
 	end
 end
 return false--found no mob
end

function movebox()
		local b = slidebox
		pushto=mget(b.x+b.dx,b.y+b.dy)
		--if not wall
		if not fget(pushto,0) then
			p_t=0
			b.x+=b.dx--SET NEW DESTINATION
			b.y+=b.dy
			b.sox=-b.dx*8--STARTING OFFSET
			b.soy=-b.dy*8
			b.ox,b.oy=b.sox,b.soy
			
			--set box turn???
			_upd=boxloop
			slidebox=b
			sfx(58)
		else
			sfx(63)
		end
end

function boxloop()
	btn_buffer()
	p_t=min(p_t+.3,1)
	boxslide(slidebox,p_t)
	if p_t ==1 then
		_upd=update_game
		slidebox=nil
	end
end

function boxslide(b,p_t)
	b.ox=b.sox*(1-p_t)
	b.oy=b.soy*(1-p_t)	
end
