--draw

function _draw()
	_drw()
	drawind()
	for txt in all(debug) do
		print(txt)
	end
end

function draw_game()
	cls()
	map()
	
	for b in all(box) do
		drawbox(b)
	end
	
 drawspr(getframe(p_ani),p_x*8+p_ox,p_y*8+p_oy,p_flp)
end

function drawspr(_spr,_x,_y,_flp)
	--change transparency colors
	palt(0,false)
	palt(14,true)
	spr(_spr,_x,_y,1,1,_flp)
	--return transparency colors
	palt(0,true)
	palt(14,false)
end

function drawbox(b)
 	drawspr(b.tle,b.x*8+b.ox,b.y*8+b.oy)
end


