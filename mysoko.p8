function _init()
	t=0
	p_ani={112,113,114,115}
	--⬅️➡️⬆️⬇️
	dirx={-1,1,0,0,1,1,-1,-1}
	diry={0,0,-1,1,-1,1,1,-1}
	
	box={}
	slidebox=nil
	_upd=update_game --the state
	_drw=draw_game
	startgame()
end

function startgame()
	buttbuff=-1
	p_x,p_y=3,3
	p_ox,p_oy=0,0--player offset
	
	p_flp=false
	p_mov=nil
	p_t=0--animation timer (0 to 1)
	--window array
	wind={}
	talkwind=nil
	
	debug={}
	
	
	for x=0,15 do
		for y=0,15 do
			tle=mget(x,y)
			--a box tile
			if 64<=tle and tle<=79 then
				addbox(tle,x,y)
				mset(x,y,1)--set ground tile
			end
		end
	end
end




