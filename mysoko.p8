--update

function _update()
	t+=1--where we are in the anim
	_upd()
end

function update_game()
	if talkwind then
 	if	get_input()==5 then
			talkwind.dur=0--close box
			talkwind=nil
		end
	else
		btn_buffer()
		do_btn_buffer(buttbuff)
		buttbuff=-1
	end
end


